{
  "name": "warpsurf",
  "version": "1.5.1",
  "description": "Browser copilot for Chrome",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/warpsurf/warpsurf.git"
  },
  "type": "module",
  "scripts": {
    "clean:bundle": "rimraf dist && turbo clean:bundle",
    "clean": "pnpm clean:bundle && rimraf .turbo",
    "prebuild": "(pnpm generate-pricing-cache || echo 'Warning: Failed to update pricing cache') && (pnpm generate-latency-cache || echo 'Warning: Failed to update latency cache')",
    "build": "pnpm clean:bundle && turbo ready && turbo build",
    "build:dev": "cross-env LOG_LEVEL=debug pnpm -w run build",
    "build:store": "(pnpm generate-pricing-cache || true) && (pnpm generate-latency-cache || true) && pnpm clean:bundle && turbo ready && cross-env __STORE__=true LOG_LEVEL=error turbo build",
    "build:api": "pnpm clean:bundle && turbo ready && cross-env __API__=true turbo build",
    "zip": "pnpm build && pnpm -F zipper zip",
    "zip:store": "pnpm build:store && cross-env __STORE__=true pnpm -F zipper zip",
    "dev": "turbo ready && cross-env __DEV__=true turbo watch dev --concurrency 20",
    "type-check": "turbo type-check",
    "lint": "turbo lint --continue -- --fix --cache --cache-location node_modules/.cache/.eslintcache",
    "prettier": "turbo prettier --continue -- --cache --cache-location node_modules/.cache/.prettiercache",
    "prepare": "husky",
    "update-version": "bash update_version.sh",
    "generate-pricing-cache": "npx tsx live_data/generate-pricing-cache.ts",
    "generate-latency-cache": "npx tsx live_data/generate-latency-cache.ts"
  },
  "dependencies": {
    "eslint-plugin-tailwindcss": "^3.18.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.326",
    "@types/node": "^22.19.2",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.12",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.2.4",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.4.18",
    "tslib": "^2.8.1",
    "tsx": "^4.19.0",
    "turbo": "^2.6.3",
    "typescript": "5.5.4",
    "vite": "6.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=22.12.0"
  },
  "pnpm": {
    "overrides": {
      "cross-spawn": "^7.0.5",
      "esbuild": "^0.25.1",
      "nanoid": "3.3.11",
      "form-data": ">=4.0.4",
      "mdast-util-to-hast": ">=13.2.1",
      "fast-xml-parser": ">=5.3.4",
      "@langchain/core": ">=0.3.80"
    }
  }
}
